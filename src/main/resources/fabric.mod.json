{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",
  "name": "${name}",
  "description": "Various mod integrations, fixes and tweaks",
  "authors": [
    "VesMaybeVesper"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "LGPL-3.0",
  "icon": "assets/vcc/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "vesper.vcc.VCC"
    ],
    "client": [
      "vesper.vcc.VCCClient"
    ]
  },
  "mixins": [
    "vcc.mixins.json",
    {
      "config": "vcc.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=0.15",
    "minecraft": "${mcdep}",
    "fabric-api": "*"
  }
}